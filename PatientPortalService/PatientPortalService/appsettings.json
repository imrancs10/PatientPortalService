{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": { "pathFormat": "logs/log-{Date}.txt" }
      }
    ]
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "MessageWaitTimeoutInMinutes": 1,
  "ConnectionStrings": {
    "MobileServiceDatabase": "Server=.\\;Database=PatientPortal;Trusted_Connection=True;"
  },
  "OnSolveMobileServiceUri": "http://localhost:7012"
}
